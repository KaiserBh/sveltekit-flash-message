{
  "name": "sveltekit-flash-message",
  "version": "2.0.0",
  "author": "Andreas SÃ¶derlund <ciscoheat@gmail.com> (https://blog.encodeart.dev)",
  "description": "Send temporary data to the next request after redirect. Works with both SSR and client.",
  "keywords": [
    "sveltekit",
    "svelte",
    "session",
    "flash message",
    "redirect",
    "post"
  ],
  "license": "MIT",
  "bugs": "https://github.com/ciscoheat/sveltekit-flash-message/issues",
  "repository": {
    "url": "https://github.com/ciscoheat/sveltekit-flash-message"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.20.5",
    "@sveltejs/package": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.31.0",
    "esm-env": "^1.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.1.12",
    "sass": "^1.63.6",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "svelte-french-toast": "1.0.4-beta.0",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "@sveltejs/kit": "1.x",
    "svelte": "3.x || 4.x"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    ">4.0": {
      "client": [
        "./dist/client.d.ts"
      ],
      "server": [
        "./dist/server.d.ts"
      ]
    }
  }
}
