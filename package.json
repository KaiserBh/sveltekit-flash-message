{
  "name": "sveltekit-flash-message",
  "version": "0.11.3",
  "author": "Andreas SÃ¶derlund <ciscoheat@gmail.com> (https://blog.encodeart.dev)",
  "description": "Send temporary data to the next request after redirect. Works with both SSR and client.",
  "keywords": [
    "sveltekit",
    "svelte",
    "session",
    "flash message"
  ],
  "license": "MIT",
  "repository": {
    "url": "https://github.com/ciscoheat/sveltekit-flash-message"
  },
  "scripts": {
    "dev": "vite dev --open",
    "build": "svelte-kit sync && svelte-package -o package",
    "preview": "vite build && vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.1",
    "@sveltejs/kit": "^1.15.9",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "esm-env": "^1.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "sass": "^1.62.1",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0",
    "svelte": "^3"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "types": "./package/client.d.ts",
      "default": "./package/client.js"
    },
    "./server": {
      "types": "./package/server.d.ts",
      "default": "./package/server.js"
    }
  },
  "files": [
    "package"
  ],
  "typesVersions": {
    ">4.0": {
      "client": [
        "./package/client.d.ts"
      ],
      "cookie-es-main": [
        "./package/cookie-es-main/index.d.ts"
      ],
      "cookie-es-main/types": [
        "./package/cookie-es-main/types.d.ts"
      ],
      "server": [
        "./package/server.d.ts"
      ]
    }
  }
}
